  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.5.x",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "browser-sync": "2.26.x",
    "core-js": "^3.6.4",
    "eslint": "4.16.0",
    "eslint-config-htmlacademy": "0.4.x",
    "gulp": "4.0.x",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-inject": "^5.0.5",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "posthtml-include": "^1.2.0",
    "stylelint": "9.10.x",
    "stylelint-config-htmlacademy": "0.1.x",
    "webpack": "^4.42.1",
    "webpack-stream": "^5.2.1"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp start",
    "webp": "gulp webp",
    "sprite": "gulp sprite",
    "deploy": "git push origin master && git subtree --prefix build push origin gh-pages",
    "server": "node server.js"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.15"
  },
  "dependencies": {
    "del": "^4.1.1",
    "express": "^4.17.1",
    "picturefill": "^3.0.3",
    "svg4everybody": "^2.1.9",
    "template-polyfill": "^2.0.0"
  }
